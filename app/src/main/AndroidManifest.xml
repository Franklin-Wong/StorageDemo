<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.integration.storagedemo">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginForgetActivity"></activity>
        <activity android:name=".file.FilePathActivity" />
        <activity android:name=".file.FileBasicActivity" />
        <activity android:name=".sqlite.LoginSQLiteActivity" />
        <activity android:name=".sqlite.SQLiteReadActivity" />
        <activity android:name=".sqlite.SQLiteWriteActivity" />
        <activity android:name=".DatabaseActivity" />
        <activity android:name=".LoginShareActivity" />
        <activity android:name=".ShareReadActivity" />
        <activity android:name=".ShareWriteActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>